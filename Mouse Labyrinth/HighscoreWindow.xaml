<Window x:Class="Mouse_Labyrinth.HighscoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mouse_Labyrinth"
        mc:Ignorable="d"
        Title="HighscoreWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="highscoreBox" HorizontalAlignment="Left" Height="343" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="772" TextChanged="highscoreBox_TextChanged"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="70,374,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
